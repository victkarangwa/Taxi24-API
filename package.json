{
  "name": "taxi24-api",
  "version": "1.0.0",
  "description": "Taxi24 is a new startup based in Kigali. They would like to disrupt the taxi industry in Rwanda by providing a white-label solution to the existing taxi companies and hotels.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "dev-server": "nodemon --exec babel-node ./src/index.js",
    "eslint": "eslint --ignore-path .gitignore .",
    "db:seed:test": "NODE_ENV=testing npx sequelize-cli db:seed:undo:all && NODE_ENV=testing sequelize db:seed:all",
    "db:seed:dev": "npx sequelize-cli db:seed:undo:all && sequelize db:seed:all",
    "test": "NODE_ENV=testing nyc --reporter=text --reporter=html --require @babel/polyfill --require @babel/register mocha ./src/specs/* --timeout 200000 --exit",
    "db:migrate": "sequelize db:migrate",
    "dropTables": "sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victkarangwa/Taxi24-API.git"
  },
  "author": "Victor Karangwa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victkarangwa/Taxi24-API/issues"
  },
  "homepage": "https://github.com/victkarangwa/Taxi24-API#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.12.10",
    "@types/node": "^14.14.22",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.0",
    "helmet": "^4.4.1",
    "http-status": "^1.5.0",
    "nodemailer": "^6.4.17",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^5.5.1",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
